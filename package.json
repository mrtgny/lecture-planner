{
  "name": "lecture-planner",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "APP_ENV=development next dev",
    "dev-prod": "APP_ENV=production next dev",
    "build": "rm -rf out .next && APP_ENV=development next build",
    "build-prod": "rm -rf out .next && APP_ENV=production next build",
    "format": "npx prettier --write \"**/*.{ts,tsx}\"",
    "start": "next start",
    "export": "npm run build && next export",
    "export-prod": "npm run build-prod && next export",
    "prepare": "husky install",
    "lint": "next lint --dir ./src/components/OptionsModal"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.4",
    "@types/react-color": "^3.0.6",
    "autoprefixer": "^10.4.7",
    "classnames": "^2.3.1",
    "next": "12.1.6",
    "postcss": "^8.4.14",
    "react": "18.2.0",
    "react-color": "^2.19.3",
    "react-dom": "18.2.0",
    "react-icons": "^4.4.0",
    "react-redux": "^8.0.2",
    "react-spring": "^9.5.2",
    "react-use-gesture": "^9.1.3",
    "tailwindcss": "^3.1.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.0",
    "@commitlint/config-conventional": "^17.4.4",
    "@types/node": "18.0.0",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "8.18.0",
    "eslint-config-next": "12.1.6",
    "husky": "^8.0.3",
    "ts-node": "^10.9.1",
    "typescript": "4.7.4"
  }
}
